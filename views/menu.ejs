<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Menu</title>
</head>
<body>
    <%- include('./partials/nav') %>

  <h1>Full Menu</h1>

  
  <ul>
    <% RESTAURANT.menu.forEach((item) => { %>
      <li>Name: <%= item.name %> -
        Price: <%= item.price %> -
        Rating: <%= item.rating %>-
        Details: <%= item.details %></li>
    <% }); %>
  </ul>
  
</body>
</html>
